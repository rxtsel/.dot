{
  "vim_mode": true,
  "relative_line_numbers": true,
  "ui_font_size": 18,
  "buffer_font_size": 18,
  "agent_font_size": 18,
  "ui_font_family": "CaskaydiaCove Nerd Font",
  "buffer_font_family": "CaskaydiaCove Nerd Font",
  "ui_font_fallbacks": ["Zed Plex Mono"],
  "buffer_font_fallbacks": ["Zed Plex Mono"],
  "buffer_line_height": "standard",
  "restore_on_startup": "last_session",
  "auto_install_extensions": {
    "html": true,
    "astro": true
  },
  "scrollbar": {
    "show": "never"
  },
  "tab_bar": {
    "show": true,
    "show_nav_history_buttons": false,
    "show_tab_bar_buttons": false
  },
  "tabs": {
    "file_icons": true,
    "git_status": true,
    "activate_on_close": "left_neighbour",
    "show_close_button": "hover"
  },
  "toolbar": {
    "breadcrumbs": false,
    "quick_actions": false
  },
  "diagnostics": {
    "include_warnings": true,
    "inline": {
      "enabled": true
    },
    "update_with_cursor": false,
    "primary_only": false,
    "use_rendered": false
  },
  "icon_theme": {
    "mode": "system",
    "dark": "Symbols",
    "light": "Symbols"
  },
  "tab_size": 2,
  "terminal": {
    "button": false,
    "font_family": "CaskaydiaCove Nerd Font",
    "font_weight": 300,
    "toolbar": {
      "breadcrumbs": false
    },
    "shell": "system",
    "line_height": {
      "custom": 1.2
    }
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Solarized Dark"
  },
  "project_panel": {
    "button": false,
    "dock": "right",
    "entry_spacing": "standard",
    "default_width": 440,
    "indent_guides": {
      "show": "never"
    },
    "auto_reveal_entries": false
  },
  "agent": {
    "button": true,
    "model_parameters": [],
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-sonnet-4-thinking-latest"
    },
    "version": "2"
  },
  "outline_panel": {
    "button": false
  },
  "collaboration_panel": {
    "button": false
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "edit_predictions": {
    "disabled_globs": [
      "**/node_modules/**",
      "**/vendor/**",
      "**/dist/**",
      "**/build/**",
      "**/out/**",
      "**/.git/**",
      "**/.hg/**",
      "**/.svn/**",
      "**/.idea/**",
      "**/.vscode/**",
      "**/.DS_Store",
      "**/.*ignore"
    ],
    "mode": "eager",
    "copilot": {
      "proxy": null,
      "proxy_no_verify": null
    },
    "enabled_in_text_threads": false
  }
}
